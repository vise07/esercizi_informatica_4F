public class Main {
    public static void main(String[] args) {

        Automobile a1 = new Automobile("lambo", "Aventador", 230000, 2);
        Moto m1 = new Moto("BMW", "rr", 12000, 1000);

        /*System.out.println(a1);
        System.out.println(m1);*/

        Concessionaria concessionaria = new Concessionaria("Fast");

        try {
            concessionaria.addVeicolo(a1);
            concessionaria.addVeicolo(m1);
        } catch (Exception e) {
            System.out.println(e.getMessage());
        }

        concessionaria.stampaConcessionaria();

        System.out.println("La concessionaria Ã¨ composta da " + concessionaria.contNumConcessionaria() + " veicoli.");
    }
}
