import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        // Costruttore di default = inizializza le variabili
        // Studente studente1 = new Studente();
        
        // Costruttore 1
        Studente Studente1 = new Studente("Pippo");
        System.out.println(Studente1.getNome());
        
        //Costruttore 2
        Studente Studente2 = new Studente("Pippo", "Marchettos");
